{
  "author": "Ben Black @bamblack",
  "bin": {
    "advent": "./bin/run.js"
  },
  "dependencies": {
    "@inquirer/prompts": "^3.3.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "axios": "^1.6.2",
    "fs-extra": "^11.2.0"
  },
  "description": "A simple CLI to interact with Advent of Code",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8.55.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^3.17.2",
    "shx": "^0.3.4",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/bamblack/advent",
  "license": "MIT",
  "main": "dist/index.js",
  "name": "advent",
  "oclif": {
    "bin": "advent",
    "dirname": "advent",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "config": {
        "description": "Manage advent config variables"
      }
    }
  },
  "repository": "bamblack/advent",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.0.0",
  "bugs": "https://github.com/bamblack/advent/issues",
  "keywords": [
    "oclif"
  ],
  "type": "commonjs",
  "types": "dist/index.d.ts"
}
